CREATE TABLE invoice
(
    InvoiceID VARCHAR(16) NOT NULL,
    InvoiceNumber VARCHAR(16) NOT NULL,
    Date DATETIME NOT NULL,
    BoxQuantity INT NOT NULL,
    T_start DATETIME NOT NULL,
    T_end DATETIME NOT NULL,
    LastModBy VARCHAR(16) NOT NULL,
    IsEnabled TINYINT DEFAULT 1 NOT NULL,
    PRIMARY KEY (InvoiceID, InvoiceNumber, LastModBy),
    FOREIGN KEY (LastModBy) REFERENCES users (UserID) ON DELETE CASCADE ON UPDATE CASCADE
);
CREATE UNIQUE INDEX InvoiceID ON invoice (InvoiceID);
